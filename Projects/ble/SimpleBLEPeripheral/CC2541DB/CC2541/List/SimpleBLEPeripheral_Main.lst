     88          
     89          	/* Initialize the operating system */
     90          	osal_init_system();
     91          
     92          	/* Enable interrupts */HAL_ENABLE_INTERRUPTS();
     93          
     94          	// Final board initialization
     95          	InitBoard(OB_READY);
     96          
     97          #if defined ( POWER_SAVING )
     98          	osal_pwrmgr_device( PWRMGR_BATTERY );
     99          #endif
    100          
    101          	/* Start OSAL */
    102          	osal_start_system(); // No Return from here
    103          
    104          	return 0;
