    107          void DS18B20_Init(void)
    108          {
    109              SET_OUT();
    110              SET_DQ();         //IO口拉高
    111              CL_DQ();          //IO口拉低
    112              delay_nus(550);   //IO拉低后保持一段时间 480-960us
    113              SET_DQ();         //释放
