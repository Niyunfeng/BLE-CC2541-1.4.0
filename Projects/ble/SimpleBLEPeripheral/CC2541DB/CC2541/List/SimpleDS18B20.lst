    119              while(DQ)
    120              {
    121                  ; 
    122              }
    123              delay_nus(240);   //检测到DQ 变低后，延时60-240us
    124              SET_OUT();        //设置IO方向为输出 CC2540->DS18B20
    125              SET_DQ();         //IO拉高
    126          }
