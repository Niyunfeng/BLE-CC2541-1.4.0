    192          uint8* IntToStr(uint8* buf, int16 m)
    193          {
    194              int8 tmp[16];
    195              int32 isNegtive = 0;
    196              int32 index;
    197          
    198              if(m < 0)
    199              {
    200                  isNegtive = 1;
    201                  m = - m;
    202              }
    203          
    204              tmp[15] = '\0';
    205              index = 14;
    206              do 
    207              {
    208                  tmp[index--] = m % 10 + '0';
    209                  m /= 10;
    210              } while (m > 0);
    211          
    212              if(isNegtive)
    213                  tmp[index--] = '-';
    214              
    215              strcpy((char *)buf, (char *)tmp + index + 1);
    216          
    217              return buf;
    218          }

