     14           *    延时函数
     15           *    输入微妙
     16           */
     17          void delay_nus(uint16 timeout)
     18          {
     19             
     20              while (timeout--)
     21              {
     22                  asm("NOP");
     23                  asm("NOP");
     24                  asm("NOP");
     25              }
     26          }
