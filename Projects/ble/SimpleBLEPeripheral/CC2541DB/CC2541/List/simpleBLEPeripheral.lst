    950           * @fn      bdAddr2Str
    951           *
    952           * @brief   Convert Bluetooth address to string. Only needed when
    953           *          LCD display is used.
    954           *
    955           * @return  none
    956           */
    957          char *bdAddr2Str(uint8 *pAddr) {
    958          	uint8 i;
    959          	char hex[] = "0123456789ABCDEF";
    960          	static char str[B_ADDR_STR_LEN];
    961          	char *pStr = str;
    962          
    963          	*pStr++ = '0';
    964          	*pStr++ = 'x';
    965          
    966          	// Start from end of addr
    967          	pAddr += B_ADDR_LEN;
    968          
    969          	for (i = B_ADDR_LEN; i > 0; i--) {
    970          		*pStr++ = hex[*--pAddr >> 4];
    971          		*pStr++ = hex[*pAddr & 0x0F];
    972          	}
    973          
    974          	*pStr = 0;
    975          
    976          	return str;
    977          }
