    761          /*********************************************************************
    762           * @fn      performPeriodicTask
    763           *
    764           * @brief   Perform a periodic application task. This function gets
    765           *          called every five seconds as a result of the SBP_PERIODIC_EVT
    766           *          OSAL event. In this example, the value of the third
    767           *          characteristic in the SimpleGATTProfile service is retrieved
    768           *          from the profile, and then copied into the value of the
    769           *          the fourth characteristic.
    770           *
    771           * @param   none
    772           *
    773           * @return  none
    774           */
    775          static void performPeriodicTask(void) {
    776          	uint8 valueToCopy;
    777          	uint8 stat;
    778          
    779          	// Call to retrieve the value of the third characteristic in the profile
    780          	stat = SimpleProfile_GetParameter(SIMPLEPROFILE_CHAR3, &valueToCopy);
    781          
    782          	if (stat == SUCCESS) {
    783          		/*
    784          		 * Call to set that value of the fourth characteristic in the profile. Note
    785          		 * that if notifications of the fourth characteristic have been enabled by
    786          		 * a GATT client device, then a notification will be sent every time this
    787          		 * function is called.
    788          		 */
    789          		SimpleProfile_SetParameter(SIMPLEPROFILE_CHAR4, sizeof(uint8), &valueToCopy);
    790          	}
    791          }
