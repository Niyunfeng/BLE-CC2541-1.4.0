    834          /*********************************************************************
    835           * @fn      performPeriodicTask
    836           *
    837           * @brief   Perform a periodic application task. This function gets
    838           *          called every five seconds as a result of the SBP_PERIODIC_EVT
    839           *          OSAL event. In this example, the value of the third
    840           *          characteristic in the SimpleGATTProfile service is retrieved
    841           *          from the profile, and then copied into the value of the
    842           *          the fourth characteristic.
    843           *
    844           * @param   none
    845           *
    846           * @return  none
    847           */
    848          static void performPeriodicTask(void) {
    849          	uint8 valueToCopy;
    850          	uint8 stat;
    851          
    852          	// Call to retrieve the value of the third characteristic in the profile
    853          	stat = SimpleProfile_GetParameter(SIMPLEPROFILE_CHAR2, &valueToCopy);
    854          
    855          	if (stat == SUCCESS) {
    856          		/*
    857          		 * Call to set that value of the fourth characteristic in the profile. Note
    858          		 * that if notifications of the fourth characteristic have been enabled by
    859          		 * a GATT client device, then a notification will be sent every time this
    860          		 * function is called.
    861          		 */
    862                         // HalLcdWriteString("ok", HAL_LCD_LINE_5);
    863          		SimpleProfile_SetParameter(SIMPLEPROFILE_CHAR4, sizeof(uint8), &valueToCopy);
    864          	}
    865          }
