    748          /*********************************************************************
    749           * @fn      peripheralStateNotificationCB
    750           *
    751           * @brief   Notification from the profile of a state change.
    752           *
    753           * @param   newState - new state
    754           *
    755           * @return  none
    756           */
    757          static void peripheralStateNotificationCB(gaprole_States_t newState) {
    758          
    759          	//HalLcdWriteStringValue("newState", newState, 10, HAL_LCD_LINE_4);
    760          
    761          	switch (newState) {
    762          	case GAPROLE_STARTED: {
    763          		uint8 ownAddress[B_ADDR_LEN];
    764          		uint8 systemId[DEVINFO_SYSTEM_ID_LEN];
    765          
    766          		GAPRole_GetParameter(GAPROLE_BD_ADDR, ownAddress);
    767          
    768          		// use 6 bytes of device address for 8 bytes of system ID value
    769          		systemId[0] = ownAddress[0];
    770          		systemId[1] = ownAddress[1];
    771          		systemId[2] = ownAddress[2];
    772          
    773          		// set middle bytes to zero
    774          		systemId[4] = 0x00;
    775          		systemId[3] = 0x00;
    776          
    777          		// shift three bytes up
    778          		systemId[7] = ownAddress[5];
    779          		systemId[6] = ownAddress[4];
    780          		systemId[5] = ownAddress[3];
    781          
    782          		DevInfo_SetParameter(DEVINFO_SYSTEM_ID, DEVINFO_SYSTEM_ID_LEN, systemId);
    783          
    784          		// Display device address
    785          		bdAddr2Str(ownAddress);
    786          		HalLcdWriteString(bdAddr2Str(ownAddress), HAL_LCD_LINE_3);
    787          		HalLcdWriteString("Initialized", HAL_LCD_LINE_3);
    788          	}
    789          		break;
    790          
    791          	case GAPROLE_ADVERTISING: {
    792          		HalLcdWriteString("Advertising", HAL_LCD_LINE_3);
    793          	}
    794          		break;
    795          
    796          	case GAPROLE_CONNECTED: {
    797          		HalLcdWriteString("Connected", HAL_LCD_LINE_3);
    798          		//simpleProfile_StateNotify( uint16 connHandle, attHandleValueNoti_t *pNoti )
    799          		GAPRole_GetParameter(GAPROLE_CONNHANDLE, &gapConnHandle);
    800          	}
    801          		break;
    802          
    803          	case GAPROLE_WAITING: {
    804          		HalLcdWriteString("Disconnected", HAL_LCD_LINE_3);
    805          	}
    806          		break;
    807          
    808          	case GAPROLE_WAITING_AFTER_TIMEOUT: {
    809          		HalLcdWriteString("Timed Out", HAL_LCD_LINE_3);
    810          	}
    811          		break;
    812          
    813          	case GAPROLE_ERROR: {
    814          		HalLcdWriteString("Error", HAL_LCD_LINE_3);
    815          	}
    816          		break;
    817          
    818          	default: {
    819          		HalLcdWriteString("", HAL_LCD_LINE_3);
    820          	}
    821          		break;
    822          
    823          	}
    824          
    825          	gapProfileState = newState;
    826          
    827          #if !defined( CC2540_MINIDK )
    828          	VOID gapProfileState; // added to prevent compiler warning with
    829          						  // "CC2540 Slave" configurations
