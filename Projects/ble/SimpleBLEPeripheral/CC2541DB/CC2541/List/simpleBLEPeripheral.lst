    675          /*********************************************************************
    676           * @fn      peripheralStateNotificationCB
    677           *
    678           * @brief   Notification from the profile of a state change.
    679           *
    680           * @param   newState - new state
    681           *
    682           * @return  none
    683           */
    684          static void peripheralStateNotificationCB(gaprole_States_t newState) {
    685          
    686          	//HalLcdWriteStringValue("newState", newState, 10, HAL_LCD_LINE_4);
    687          
    688          	switch (newState) {
    689          	case GAPROLE_STARTED: {
    690          		uint8 ownAddress[B_ADDR_LEN];
    691          		uint8 systemId[DEVINFO_SYSTEM_ID_LEN];
    692          
    693          		GAPRole_GetParameter(GAPROLE_BD_ADDR, ownAddress);
    694          
    695          		// use 6 bytes of device address for 8 bytes of system ID value
    696          		systemId[0] = ownAddress[0];
    697          		systemId[1] = ownAddress[1];
    698          		systemId[2] = ownAddress[2];
    699          
    700          		// set middle bytes to zero
    701          		systemId[4] = 0x00;
    702          		systemId[3] = 0x00;
    703          
    704          		// shift three bytes up
    705          		systemId[7] = ownAddress[5];
    706          		systemId[6] = ownAddress[4];
    707          		systemId[5] = ownAddress[3];
    708          
    709          		DevInfo_SetParameter(DEVINFO_SYSTEM_ID, DEVINFO_SYSTEM_ID_LEN, systemId);
    710          
    711          		// Display device address
    712          		bdAddr2Str(ownAddress);
    713          		HalLcdWriteString(bdAddr2Str(ownAddress), HAL_LCD_LINE_3);
    714          		HalLcdWriteString("Initialized", HAL_LCD_LINE_3);
    715          	}
    716          		break;
    717          
    718          	case GAPROLE_ADVERTISING: {
    719          		HalLcdWriteString("Advertising", HAL_LCD_LINE_3);
    720          	}
    721          		break;
    722          
    723          	case GAPROLE_CONNECTED: {
    724          		HalLcdWriteString("Connected", HAL_LCD_LINE_3);
    725          		//simpleProfile_StateNotify( uint16 connHandle, attHandleValueNoti_t *pNoti )
    726          		GAPRole_GetParameter(GAPROLE_CONNHANDLE, &gapConnHandle);
    727          	}
    728          		break;
    729          
    730          	case GAPROLE_WAITING: {
    731          		HalLcdWriteString("Disconnected", HAL_LCD_LINE_3);
    732          	}
    733          		break;
    734          
    735          	case GAPROLE_WAITING_AFTER_TIMEOUT: {
    736          		HalLcdWriteString("Timed Out", HAL_LCD_LINE_3);
    737          	}
    738          		break;
    739          
    740          	case GAPROLE_ERROR: {
    741          		HalLcdWriteString("Error", HAL_LCD_LINE_3);
    742          	}
    743          		break;
    744          
    745          	default: {
    746          		HalLcdWriteString("", HAL_LCD_LINE_3);
    747          	}
    748          		break;
    749          
    750          	}
    751          
    752          	gapProfileState = newState;
    753          
    754          #if !defined( CC2540_MINIDK )
    755          	VOID gapProfileState; // added to prevent compiler warning with
    756          						  // "CC2540 Slave" configurations
    757          #endif
    758          
    759          }
