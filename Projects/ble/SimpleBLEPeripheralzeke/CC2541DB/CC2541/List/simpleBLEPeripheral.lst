    685          /*********************************************************************
    686           * @fn      performPeriodicTask
    687           *
    688           * @brief   Perform a periodic application task. This function gets
    689           *          called every five seconds as a result of the SBP_PERIODIC_EVT
    690           *          OSAL event. In this example, the value of the third
    691           *          characteristic in the SimpleGATTProfile service is retrieved
    692           *          from the profile, and then copied into the value of the
    693           *          the fourth characteristic.
    694           *
    695           * @param   none
    696           *
    697           * @return  none
    698           */
    699          static void performPeriodicTask(void) {
    700          	uint8 valueToCopy;
    701          	uint8 stat;
    702          
    703          	// Call to retrieve the value of the third characteristic in the profile
    704          	stat = SimpleProfile_GetParameter(SIMPLEPROFILE_CHAR3, &valueToCopy);
    705          
    706          	if (stat == SUCCESS) {
    707          		/*
    708          		 * Call to set that value of the fourth characteristic in the profile. Note
    709          		 * that if notifications of the fourth characteristic have been enabled by
    710          		 * a GATT client device, then a notification will be sent every time this
    711          		 * function is called.
    712          		 */
    713          		SimpleProfile_SetParameter(SIMPLEPROFILE_CHAR4, sizeof(uint8), &valueToCopy);
    714          	}
    715          }
