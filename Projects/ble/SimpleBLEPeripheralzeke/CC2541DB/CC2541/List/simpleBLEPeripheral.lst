    799          /*********************************************************************
    800           * @fn      bdAddr2Str
    801           *
    802           * @brief   Convert Bluetooth address to string. Only needed when
    803           *          LCD display is used.
    804           *
    805           * @return  none
    806           */
    807          char *bdAddr2Str(uint8 *pAddr) {
    808          	uint8 i;
    809          	char hex[] = "0123456789ABCDEF";
    810          	static char str[B_ADDR_STR_LEN];
    811          	char *pStr = str;
    812          
    813          	*pStr++ = '0';
    814          	*pStr++ = 'x';
    815          
    816          	// Start from end of addr
    817          	pAddr += B_ADDR_LEN;
    818          
    819          	for (i = B_ADDR_LEN; i > 0; i--) {
    820          		*pStr++ = hex[*--pAddr >> 4];
    821          		*pStr++ = hex[*pAddr & 0x0F];
    822          	}
    823          
    824          	*pStr = 0;
    825          
    826          	return str;
    827          }
