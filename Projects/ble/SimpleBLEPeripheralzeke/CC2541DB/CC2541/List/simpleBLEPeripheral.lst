    717          /*********************************************************************
    718           * @fn      simpleProfileChangeCB
    719           * @brief   Callback from SimpleBLEProfile indicating a value change
    720           * @param   paramID - parameter ID of the value that was changed.
    721           *
    722           * @return  none
    723           */
    724          static void simpleProfileChangeCB(uint8 paramID) {
    725          	osal_memset(newValueBuf, 0, 20);
    726          	switch (paramID) {
    727          	case SIMPLEPROFILE_CHAR1:
    728          		SimpleProfile_GetParameter(SIMPLEPROFILE_CHAR1, newValueBuf);
    729          
    730          		if ((newValueBuf[0] == TRANSFER_DATA_SIGN) && (newValueBuf[1] == TRANSFER_DATA_SIGN_RE)) {
    731          			TRANSFER_DATA_STATE_IN = FALSE;
    732          		}
    733          
    734          		if ((newValueBuf[2] != 0) && (!TRANSFER_DATA_STATE_IN)) {
    735          			data_len = newValueBuf[UART_DATA_START_INDEX];
    736          			TRANSFER_DATA_STATE_IN = TRUE;
    737          			data_len_index = 0;
    738          			osal_memset(recv_value, 0, data_len);
    739          		}
    740          
    741          		cur_data_len = osal_strlen(newValueBuf);
    742          
    743          		if (TRANSFER_DATA_STATE_IN) {
    744          			osal_memcpy((recv_value + data_len_index), newValueBuf, cur_data_len);
    745          			data_len_index += cur_data_len;
    746          
    747          			if ((data_len_index - send_times * one_time_data_len - UART_DATA_START_INDEX) >= one_time_data_len) {
    748          				if (send_times == 0) {
    749          					recv_value[UART_DATA_START_INDEX] = 0xE3;
    750          					//if (u_state != IR_DATA_SEND_BEGIN_STATE) {
    751          					//u_state = IR_DATA_SEND_BEGIN_STATE;
    752          					SbpHalUARTWrite(recv_value + UART_DATA_START_INDEX, one_time_data_len);
    753          					//}
    754          				} else {
    755          					SbpHalUARTWrite(recv_value + UART_DATA_START_INDEX + send_times * one_time_data_len,
    756          							one_time_data_len);
    757          				}
    758          				send_times++;
    759          			} else if ((send_times > 0) && ((data_len_index - send_times * one_time_data_len) < one_time_data_len)
    760          					&& (data_len_index == data_len)) {
    761          
    762          				SbpHalUARTWrite(recv_value + UART_DATA_START_INDEX + send_times * one_time_data_len,
    763          						data_len - send_times * one_time_data_len - UART_DATA_START_INDEX);
    764          				send_times++;
    765          
    766          			} else if ((send_times == 0) && (data_len < one_time_data_len) && (data_len_index == data_len)) {
    767          				recv_value[UART_DATA_START_INDEX] = 0xE3;
    768          				//if (u_state != IR_DATA_SEND_BEGIN_STATE) {
    769          				//u_state = IR_DATA_SEND_BEGIN_STATE;
    770          				SbpHalUARTWrite(recv_value + UART_DATA_START_INDEX, data_len - UART_DATA_START_INDEX);
    771          				//}
    772          			} else {
    773          
    774          			}
    775          		}
    776          
    777          		HalLcdWriteStringValue("data_len_index:", data_len_index, 10, HAL_LCD_LINE_7);
    778          
    779          		if (data_len_index == data_len) {
    780          			TRANSFER_DATA_STATE_IN = FALSE;
    781          			HalLcdWriteStringValue("data_len:", osal_strlen((char *) recv_value), 10, HAL_LCD_LINE_6);
    782          			send_times = 0;
    783          			data_len = 0;
    784          			cur_data_len = 0;
    785          			data_len_index = 0;
    786          			osal_memset(recv_value, 0, data_len);
    787          		}
    788          
    789          		break;
    790          	case SIMPLEPROFILE_CHAR3:
    791          		//SimpleProfile_GetParameter(SIMPLEPROFILE_CHAR3, &newValue);
    792          		break;
    793          	default:
    794          		// should not reach here!
    795          		break;
    796          	}
    797          }
