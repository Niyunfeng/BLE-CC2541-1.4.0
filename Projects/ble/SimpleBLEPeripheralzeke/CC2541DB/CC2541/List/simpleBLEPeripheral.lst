    357          		uint8 enable_update_request = DEFAULT_ENABLE_UPDATE_REQUEST;
    358          		uint16 desired_min_interval = DEFAULT_DESIRED_MIN_CONN_INTERVAL;
    359          		uint16 desired_max_interval = DEFAULT_DESIRED_MAX_CONN_INTERVAL;
