    558          /*********************************************************************
    559           * @fn      simpleBLEPeripheral_ProcessOSALMsg
    560           *
    561           * @brief   Process an incoming task message.
    562           *
    563           * @param   pMsg - message to process
    564           *
    565           * @return  none
    566           */
    567          static void simpleBLEPeripheral_ProcessOSALMsg(osal_event_hdr_t *pMsg) {
    568          	switch (pMsg->event) {
    569          	case KEY_CHANGE:
    570          		simpleBLEPeripheral_HandleKeys(((keyChange_t *) pMsg)->state, ((keyChange_t *) pMsg)->keys);
    571          		break;
    572          	default:
    573          		// do nothing
    574          		break;
    575          	}
    576          }
    577          
    578          static void simpleBLEPeripheral_HandleKeys(uint8 shift, uint8 keys) {
    579          	if (keys & HAL_KEY_UP) {
    580          		u_state = IR_DATA_STUDY_CMD_START_BEGIN_STATE;
    581          		SbpHalUARTWrite(&SBP_UART_STUDY_CMD, SBP_UART_STUDY_CMD_LEN);
    582          	}
    583          
    584          	if (keys & HAL_KEY_LEFT) {
    585          		HalLcdWriteString("read rtc ...", HAL_LCD_LINE_3);
    586          	}
    587          
    588          	if (keys & HAL_KEY_DOWN) {
    589          		HalLcdWriteString("send after 3s...", HAL_LCD_LINE_4);
    590          		osal_start_timerEx(simpleBLEPeripheral_TaskID, SBP_SEND_IRDATA_EVT, 3000);
    591          	}
    592          
    593          	if (keys & HAL_KEY_RIGHT) {
    594          		HalLcdWriteStringValue("data_len:", data_len, 10, HAL_LCD_LINE_2);
    595          	}
    596          
    597          }
