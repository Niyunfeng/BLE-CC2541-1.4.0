    480          /*********************************************************************
    481           * @fn      SimpleBLEPeripheral_ProcessEvent
    482           *
    483           * @brief   Simple BLE Peripheral Application Task event processor.  This function
    484           *          is called to process all events for the task.  Events
    485           *          include timers, messages and any other user defined events.
    486           *
    487           * @param   task_id  - The OSAL assigned task ID.
    488           * @param   events - events to process.  This is a bit map and can
    489           *                   contain more than one event.
    490           *
    491           * @return  events not processed
    492           */
    493          uint16 SimpleBLEPeripheral_ProcessEvent(uint8 task_id, uint16 events) {
    494          
    495          	VOID task_id; // OSAL required parameter that isn't used in this function
    496          
    497          	if (events & SYS_EVENT_MSG) {
    498          		uint8 *pMsg;
    499          
    500          		if ((pMsg = osal_msg_receive(simpleBLEPeripheral_TaskID)) != NULL) {
    501          			simpleBLEPeripheral_ProcessOSALMsg((osal_event_hdr_t *) pMsg);
    502          
    503          			// Release the OSAL message
    504          			VOID osal_msg_deallocate(pMsg);
    505          		}
    506          
    507          		// return unprocessed events
    508          		return (events ^ SYS_EVENT_MSG);
    509          	}
    510          
    511          	if (events & SBP_START_DEVICE_EVT) {
    512          		// Start the Device
    513          		GAPRole_StartDevice(&simpleBLEPeripheral_PeripheralCBs);
    514          
    515          		// Start Bond Manager
    516          		GAPBondMgr_Register(&simpleBLEPeripheral_BondMgrCBs);
    517          
    518          		// Set timer for first periodic event
    519          		osal_start_timerEx(simpleBLEPeripheral_TaskID, SBP_PERIODIC_EVT, SBP_PERIODIC_EVT_PERIOD);
    520          
    521          		return (events ^ SBP_START_DEVICE_EVT);
    522          	}
    523          
    524          	if (events & SBP_PERIODIC_EVT) {
    525          		// Restart timer
    526          		if (SBP_PERIODIC_EVT_PERIOD) {
    527          			osal_start_timerEx(simpleBLEPeripheral_TaskID, SBP_PERIODIC_EVT, SBP_PERIODIC_EVT_PERIOD);
    528          		}
    529          
    530          		// Perform periodic application task
    531          		performPeriodicTask();
    532          
    533          		return (events ^ SBP_PERIODIC_EVT);
    534          	}
    535          
    536          	if (events & SBP_ZEKEZANG_EVT) {
    537          		uint8 initial_advertising_enable = FALSE;
    538          		GAPRole_SetParameter(GAPROLE_ADVERT_ENABLED, sizeof(uint8), &initial_advertising_enable);
    539          		return (events ^ SBP_ZEKEZANG_EVT);
    540          	}
    541          
    542          	if (events & SBP_SEND_IRDATA_EVT) {
    543          		HalLcdWriteString("send plan complete...", HAL_LCD_LINE_4);
    544          		return (events ^ SBP_SEND_IRDATA_EVT);
    545          	}
    546          
    547          	if (events & SBP_ADV_IN_CONNECTION_EVT) {
    548          		uint8 turnOnAdv = TRUE;
    549          		// Turn on advertising while in a connection
    550          		GAPRole_SetParameter(GAPROLE_ADVERT_ENABLED, sizeof(uint8), &turnOnAdv);
    551          
    552          		return (events ^ SBP_ADV_IN_CONNECTION_EVT);
    553          	}
    554          
    555          	return 0;
    556          }
