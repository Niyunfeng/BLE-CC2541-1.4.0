    664          static void simpleBLEPeripheral_ProcessOSALMsg(osal_event_hdr_t *pMsg) {
    665          	switch (pMsg->event) {
    666          	case KEY_CHANGE:
    667          		simpleBLEPeripheral_HandleKeys(((keyChange_t *) pMsg)->state, ((keyChange_t *) pMsg)->keys);
    668          		break;
    669          	default:
    670          		// do nothing
    671          		break;
    672          	}
    673          }
