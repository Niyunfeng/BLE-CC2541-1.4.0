     19          /**************************************************************** 
     20          读取温度传感器 AD 值函数       
     21          ****************************************************************/ 
     22          uint8 getTemperature(void){ 
     23             
     24            uint8 i;
     25            uint16 adcvalue;
     26            uint16 value;
     27            
     28            adcvalue=0;
     29            for(i=0;i<4;i++)
     30            {
     31              ADCCON3 |=0X0E;//单通道AD转换源为温度传感器
     32              ADCCON3 &=0X3F;//单通道AD转换参考电压为1.25内部电压
     33              ADCCON3 |=0X30;//单通道AD转换分辨率为512DEC，12有效
     34              ADCCON1 |=0X30;//ADC启动方式选择为ADCCON1.ST=1事件
     35              ADCCON1 |=0X40;//ADC启动转换
     36                  
     37              while(!ADCCON1&0X80);//等待AD转换完成
     38              value =  ADCL >> 2; //ADCL寄存器低2位无效
     39              value |=(((uint16)ADCH)<<6);//连接ADCH和ADCL，并赋值给value
     40              adcvalue +=value;//adcvalue被赋值为4次AD值之和
     41            }
     42            value=adcvalue>>2;//累加除以4，得到平均值
     43            return ((value) >> 4) - 315;     //根据AD值，计算出实际的温度
     44          }

