     49          /**************************************************************************************************
     50           * @fn          HalCRCCalc
     51           *
     52           * @brief       Calculate the H/W CRC result.
     53           *
     54           * input parameters
     55           *
     56           * None.
     57           *
     58           * output parameters
     59           *
     60           * None.
     61           *
     62           * @return      The H/W CRC result.
     63           */
     64          uint16 HalCRCCalc(void)
     65          {
     66            uint16 crc = RNDH;
     67            crc = (crc << 8) | RNDL;
     68          
     69            return crc;
     70          }
     71          
     72          /**************************************************************************************************
     73           * @fn          HalCRCExec
     74           *
     75           * @brief       Execute the H/W CRC calculation on the input byte.
     76           *
     77           * input parameters
     78           *
     79           * @param       ch - The byte on which to execute the H/W CRC calculation.
     80           *
     81           * output parameters
     82           *
     83           * None.
     84           *
     85           * @return      None.
     86           */
     87          void HalCRCExec(uint8 ch)
     88          {
     89            RNDH = ch;
     90          }
     91          
     92          /**************************************************************************************************
     93           * @fn          HalCRCInit
     94           *
     95           * @brief       Initialize the H/W for a new CRC calculation.
     96           *
     97           * input parameters
     98           *
     99           * @param       seed - The CRC seed.
    100           *
    101           * output parameters
    102           *
    103           * None.
    104           *
    105           * @return      None.
    106           */
    107          void HalCRCInit(uint16 seed)
    108          {
    109            ADCCON1 &= 0xF3;  // CRC configuration of LRSR.
    110          
    111            RNDL = HI_UINT16(seed);
    112            RNDL = LO_UINT16(seed);
    113          }
